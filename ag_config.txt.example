TEST_ENVIRONMENT = TRUE

# Path to the base directory where the data files are going to be stored
BASE_DATA_DIR =

LOCALE = american_gut
name = American Gut Project
shorthand = American Gut

# ----------------------------- Postgres settings -----------------------------
[postgres]
# The user name to connect to the database
